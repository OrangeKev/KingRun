<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="DialCircleView2">
        <attr name="text_km" format="string" />
        <attr name="text_kcal" format="string" />
        <attr name="text_time" format="string" />
        <attr name="line_width_equality" format="boolean" />
        <attr name="text_hide_circle" format="boolean" />
    </declare-styleable>


    <declare-styleable name="LineChart">
        <attr name="StrokeWidth" format="dimension" />
        <attr name="FontSize" format="dimension" />
        <attr name="PointRadius" format="dimension" />
        <attr name="DarkColor" format="color" />
        <attr name="LightColor" format="color" />
        <attr name="DateTextColor" format="color" />
        <attr name="ShapeColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CircleButtonView">
        <attr name="minTime" format="integer"/>
        <attr name="maxTime" format="integer"/>
        <attr name="progressColor" format="color"/>
        <attr name="progressWidth" format="dimension"/>
    </declare-styleable>
</resources>